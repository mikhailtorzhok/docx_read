var checkboxes = document.getElementsByClassName('iceSelBoolChkbx');
for (var i=0; i<checkboxes.length; i++) { //проходим по всем чекбоксам
  checkboxes[i].checked = true; //включаем их
}

var uncheckboxes = document.getElementById('pgzform:userInfoTabs:0:etpEpTable:etpEpTableColumns:0:00:etpEpPrivilege');
for (var i=0; i<uncheckboxes.length; i++) { //проходим по всем чекбоксам
	alert(uncheckboxes[i]);
	alert(i);
}



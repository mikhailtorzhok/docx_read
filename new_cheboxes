  var checkboxes = document.getElementsByClassName('iceSelBoolChkbx');
for (var i=0; i<checkboxes.length; ++i) { //проходим по всем чекбоксам
  checkboxes[i].checked = true; //включаем их
}
var checkboxes = document.getElementsByClassName('iceSelBoolChkbx-dis');
for (var i=0; i<checkboxes.length; ++i) { 
  checkboxes[i].disabled = false; 
  checkboxes[i].checked = true; 
}
var checkboxes = document.getElementsByClassName('iceInpTxtArea-dis');
for (var i=0; i<checkboxes.length; ++i) { 
  checkboxes[i].disabled = false; 
  checkboxes[i].value = 'Должностные обязанности';
}
for (var i=0; i<8; ++i) {
  for (var j=0; j<5; ++j) {
    if (i == 3) continue;
    var checkboxId = document.getElementById('pgzform:userInfoTabs:0:etpTable:0:etpTableColumns:'+i+':'+i+j+':etpPrivilege');
    checkboxId.checked = false;
  }
}
for (var i=0; i<9; ++i) {
  for (var j=0; j<5; ++j) {
    if (i == 3) continue;
    var checkboxId = document.getElementById('pgzform:userInfoTabs:0:etpEpTable:0:etpEpTableColumns:'+i+':'+i+j+':etpEpPrivilege');
    checkboxId.checked = false;
  }
}